#Assignment 3
n = int(input("Enter size of square matrix:"))

print("Enter elements row-wise:")
A=[]
for i in range(n):
    row = list(map(int, input().split()))
      A.append(row)

skew= True
for i in range(n):
    for j in range(n):
       if A[i][j] != -A[j][i]:
          skew= False
            break

if skew:
     print("Matrix is Skew-Symmetric")
else:
    print("Matrix is NOT Skew-Symmetric")
